{
  "title": "udk/write",
  "favicon": "/src/static/skins/medienhaus/img/favicon.svg",
  "skinName": "medienhaus",
  "ip": "0.0.0.0",
  "port": 9001,
  "dbType" : "postgres",
  "dbSettings" : {
    "host": "postgres-etherpad",
    "port": "5432",
    "database": "etherpad",
    "user": "etherpad",
    "password": "${ETHERPAD_POSTGRES_PASSWORD}"
  },
  "defaultPadText" : "",
  "padOptions": {
    "noColors": true,
    "showControls": true,
    "showChat": false,
    "showLineNumbers": true,
    "useMonospaceFont": true,
    "userName": null,
    "userColor": null,
    "rtl": false,
    "alwaysShowChat": false,
    "chatAndUsers": false,
    "lang": "en-us"
  },
  "trustProxy": "true",
  "cookie": {
    "sameSite": "None"
  },
  "users": {
    "admin": {
      "password": "${ETHERPAD_ADMIN_PASSWORD}",
      "is_admin": true
    }
  },
  "socketIo": {
    "maxHttpBufferSize": 10000
  }
}
