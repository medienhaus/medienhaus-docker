{
  "team_name": "udk/sketch",
  "contact_email": "",

  "host": "::",
  "port": 9666,
  "endpoint": "http://localhost:9666",

  "invite_code": "${SPACEDECK_INVITE_CODE}",
  "session_key": "${SPACEDECK_SESSION_KEY}",

  "export_api_secret": "${SPACEDECK_EXPORT_API_SECRET}",

  "storage_dialect": "postgres",
  "storage_host": "postgres-spacedeck",
  "storage_username": "spacedeck",
  "storage_password": "${SPACEDECK_POSTGRES_PASSWORD}",
  "storage_database": "spacedeck",

  "storage_local_path": "./storage",
  "storage_local_db": "./database.sqlite",
  "storage_region": "eu-central-1",
  "storage_endpoint": "http://localhost:4572",
  "storage_bucket": "my_spacedeck_bucket",
  "storage_cdn": "/storage",

  "mongodb_host": "localhost",
  "redis_mock": true,
  "redis_host": "localhost",

  "auth_type": "ldap",
  "auth_ldap_server": "${LDAP_SCHEMA}://${LDAP_HOST}:${LDAP_PORT}",
  "auth_ldap_bind_dn": "${LDAP_BIND_DN}",
  "auth_ldap_bind_credentials": "${LDAP_BIND_PASSWORD}",
  "auth_ldap_search_base": "${LDAP_BASE}",
  "auth_ldap_search_filter": "(|(${LDAP_ATTRIBUTE_MAIL}={{mail}})(${LDAP_ATTRIBUTE_UID}={{username}}))",
  "auth_ldap_search_attributes": ["${LDAP_ATTRIBUTE_UID}", "${LDAP_ATTRIBUTE_MAIL}", "${LDAP_ATTRIBUTE_NAME}"],
  "auth_ldap_attribute_uid": "${LDAP_ATTRIBUTE_UID}",
  "auth_ldap_attribute_mail": "${LDAP_ATTRIBUTE_MAIL}",
  "auth_ldap_attribute_name": "${LDAP_ATTRIBUTE_NAME}",

  "mail_provider": "smtp",
  "mail_smtp_host": "",
  "mail_smtp_port": 587,
  "mail_smtp_secure": true,
  "mail_smtp_require_tls": true,
  "mail_smtp_user": "",
  "mail_smtp_pass": "",

  "spacedeck": {}
}
